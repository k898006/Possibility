<h1>ユーザー一覧</h1>
<table>
  <thead>
    <tr>
      <th>ユーザーID</th>
      <th>ユーザーネーム</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td>
          <%= link_to admin_user_path(user) do %>
          <%= user.name %>
          <% end %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
